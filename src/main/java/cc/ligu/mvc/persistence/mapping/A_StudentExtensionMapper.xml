<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cc.ligu.mvc.persistence.dao.A_StudentExtensionMapper" >

    <resultMap id="ExtensionBaseResultMap" type="cc.ligu.mvc.persistence.entity.Student"
               extends="cc.ligu.mvc.persistence.dao.StudentMapper.BaseResultMap" >
    </resultMap>

    <select id="selectStudentByMac" resultMap="ExtensionBaseResultMap" parameterType="java.lang.String" >
        select
        <include refid="cc.ligu.mvc.persistence.dao.StudentMapper.Base_Column_List" />
        from user_student
        where zslb = #{mac,jdbcType=VARCHAR}
    </select>


</mapper>
