<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.gukeer.platform.persistence.dao.A_NetDiskShareLinkMapper">
    <insert id="batchInsertNetDiskShareLinks" >
        INSERT INTO `net_disk_share`
        (`id`,
        `school_id`,
        `teacher_id`,
        `link`,
        `shared_teacher_id`,
        `share_type`,
        `file_name`,
        `is_dir`,
        `expire_date`,
        `token`,
        `can_preview`,
        `can_downlod`,
        `is_expired`,
        `email`,
        `path`,
        `repo_name`,
        `create_time`,
        `file_size`
        )
        values
        <foreach collection="netDiskShareLinks" item="item" index="index" separator=",">
            (
            #{item.id},#{item.schoolId},#{item.teacherId},#{item.link},#{item.sharedTeacherId},#{item.shareType},#{item.fileName},
            #{item.isDir},#{item.expireDate},#{item.token},#{item.canPreview},#{item.canDownlod},#{item.isExpired},#{item.email},
            #{item.path},#{item.repoName},#{item.createTime},#{item.fileSize}
            )
        </foreach>
    </insert>

</mapper>
